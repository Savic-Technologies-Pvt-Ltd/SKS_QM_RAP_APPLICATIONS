  @UI.identification: [{ position: 10, qualifier: 'Fieldgroup6', label: 'QC Status' }]
  @Consumption.valueHelpDefinition: [{ entity:{ name: 'ZQM_VH_ILOT_QC_STS' ,element: 'Text' } }]
