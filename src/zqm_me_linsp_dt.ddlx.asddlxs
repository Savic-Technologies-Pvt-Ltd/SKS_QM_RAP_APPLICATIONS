  @UI.identification: [{ position: 100, qualifier: 'Fieldgroup4', label: 'Result 10 Acc/Rej' }]
  @Consumption.valueHelpDefinition: [{ entity:{ name: 'ZQM_BA_LINSP_ACCREJ_VH' , element: 'Text'} }]
  Result10;

  @UI.identification: [{ position: 110, qualifier: 'Fieldgroup4', label: 'Result 11 Acc/Rej' }]
  @Consumption.valueHelpDefinition: [{ entity:{ name: 'ZQM_BA_LINSP_ACCREJ_VH' , element: 'Text'} }]
