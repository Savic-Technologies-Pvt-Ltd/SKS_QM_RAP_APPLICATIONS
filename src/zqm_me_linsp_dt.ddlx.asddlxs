@Metadata.layer: #CORE
@UI: {
      headerInfo:
      {
         typeName: 'Line Inspection Lot',
         typeNamePlural: 'Line Inspection Lot',
         title:
         {
           type : #STANDARD,
           label: 'Line Inspection Lot',
           value: 'Operationdate'
         }
      }
}


annotate entity ZQM_CM_LINSP_DT with
{

  @UI.facet: [
          {  position : 10,
             id : 'DT',
                         label          : 'General Data',
             
             purpose : #STANDARD,
             type : #IDENTIFICATION_REFERENCE
          },

         {
            id             : 'Fieldgroup2ID',
            type           : #IDENTIFICATION_REFERENCE,
            label          : 'Instrument Code',
            targetQualifier: 'Fieldgroup2',
            position       : 20
          },
          {
        id            : 'SPC',
        purpose       : #STANDARD,
        type          : #LINEITEM_REFERENCE,
        label         : 'Specifications',
        position      : 30,
        targetElement : '_SPC'
          }
            ,
      {
            id             : 'Fieldgroup4ID',
            type           : #IDENTIFICATION_REFERENCE,
            label          : 'Inspection Accept/Reject',
            targetQualifier: 'Fieldgroup4',
            position       : 40
      },
          {
            id             : 'Fieldgroup5ID',
            type           : #IDENTIFICATION_REFERENCE,
            label          : 'Remarks',
            targetQualifier: 'Fieldgroup5',
            position       : 50
          },
          {
            id             : 'Fieldgroup6ID',
            type           : #IDENTIFICATION_REFERENCE,
            label          : 'QA Status',
            targetQualifier: 'Fieldgroup6',
            position       : 60
          }
  ]

  @UI.lineItem: [{ position : 10,label :'Operation Date' }]
  @UI.identification: [{  position : 10, label : 'Operation Date'}]
  @EndUserText.label: 'Operation Text'
  Operationdate;

  @UI.lineItem: [{ position : 20,label :'Shift' }]
  @UI.identification: [{  position : 20, label : 'Shift'}]
  @EndUserText.label: 'Shift'
  @Consumption.valueHelpDefinition: [{ entity : { name: 'ZQM_BA_LINSP_SHIFT_VH' , element: 'Value' } }]
  Shift;

  @UI.identification: [{ position: 30, label: 'Previous Operation' }]
  @Consumption.valueHelpDefinition: [{ entity:{ name: 'ZQM_BA_LINSP_MACHINE_VH',element: 'Opeartion' } }]
  PreviousOperation;

  @UI.identification: [{ position: 40, label: 'Next Operation' }]
  @Consumption.valueHelpDefinition: [{ entity:{ name: 'ZQM_BA_LINSP_MACHINE_VH',element: 'Opeartion' } }]
  NextOperation;

  // === Instrument Code tab (Fieldgroup2) ===
  @UI.identification: [{ position: 10, qualifier: 'Fieldgroup2', label: 'DMT' }]
  Dmt;

  @UI.identification: [{ position: 20, qualifier: 'Fieldgroup2', label: 'VCD' }]
  Vcd;

  @UI.identification: [{ position: 30, qualifier: 'Fieldgroup2', label: 'Dial' }]
